/*
 * Animation Lazy Loading
 */

.animate {
  min-height: 3px;
  opacity: 0;
  margin-right: -10px;
  margin-top: -3px;

  -webkit-animation: modal 2250ms ease-out 750ms infinite running; /* Chrome, Safari, Opera */

  -moz-animation-duration: 2250ms;
  -moz-animation-delay: 750ms;
  -moz-animation-name: modal;
  -moz-animation-iteration-count: infinite;
  -moz-animation-play-state: running;

  animation: modal 2250ms ease-out 750ms infinite running;
}

.animate-load {
  margin: auto;
  width: 10px;
  height: 3px;
  background: #ff0000;
  position: relative;

  -webkit-animation: myfirst 2250ms ease-out 0s infinite running; /* Chrome, Safari, Opera */

  -moz-animation-duration: 2250ms;
  -moz-animation-delay: 0s;
  -moz-animation-name: myfirst;
  -moz-animation-iteration-count: infinite;
  -moz-animation-direction: running;

  animation: myfirst 2250ms  ease-out 0s infinite running;

}

.animate-run {
  -webkit-animation-play-state: running; /* Chrome, Safari, Opera */
  -moz-animation-play-state: running;
  -o-animation-play-state: running;

  animation-play-state: running;
}

.animate-stop {
  -webkit-animation-play-state: paused; /* Chrome, Safari, Opera */
  -moz-animation-play-state: paused;
  -o-animation-play-state: paused;

  animation-play-state: paused;
}

.animate-page {
  opacity: 0;
  margin-top: 0px;
  padding-top: 50px;

  -webkit-animation: showpage 400ms ease-out 100ms alternate forwards; /* Chrome, Safari, Opera */

  -moz-animation-duration: 400ms;
  -moz-animation-delay: 100ms;
  -moz-animation-name: showpage;
  -moz-animation-iteration-count: alternate;
  -moz-animation-direction: running;

  animation: showpage 400ms ease-out 100ms alternate forwards;
}

.animate-show-detail {
  opacity: 0;
  margin-top: 0px;

  -webkit-animation: showdetail 400ms ease-out 100ms alternate forwards; /* Chrome, Safari, Opera */

  -moz-animation-duration: 400ms;
  -moz-animation-delay: 100ms;
  -moz-animation-name: showdetail;
  -moz-animation-iteration-count: alternate;
  -moz-animation-direction: running;

  animation: showdetail 400ms ease-out 100ms alternate forwards;
}

.fixed-page {
  position: fixed;
  top: 0px;
  left: 0px;
  right: 0px;
  bottom: 0px;
}

.content-page {
  padding-top: 20px;
}

/* Chrome, Safari, Opera */
@-webkit-keyframes showpage {
    0% {opacity: 0; margin-top: 30px;}
    100% {opacity: 1; margin-top: 0px;}
}

